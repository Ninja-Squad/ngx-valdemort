{
  "name": "ngx-valdemort",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --project ngx-valdemort",
    "watch": "ng build --project ngx-valdemort --watch --configuration development",
    "test": "ng test --no-progress --no-watch --code-coverage --project ngx-valdemort",
    "build:demo": "ng build --project demo",
    "test:demo": "ng test --no-progress --no-watch --project demo",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "node scripts/prepare-doc.js && cd projects/ngx-valdemort && compodoc",
    "release": "cd projects/ngx-valdemort && commit-and-tag-version --infile ../../CHANGELOG.md",
    "format": "prettier --write ./"
  },
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "dependencies": {
    "@angular/animations": "19.2.0",
    "@angular/cdk": "19.2.1",
    "@angular/common": "19.2.0",
    "@angular/compiler": "19.2.0",
    "@angular/core": "19.2.0",
    "@angular/forms": "19.2.0",
    "@angular/material": "19.2.1",
    "@angular/platform-browser": "19.2.0",
    "@angular/platform-browser-dynamic": "19.2.0",
    "@angular/platform-server": "19.2.0",
    "@angular/router": "19.2.0",
    "@angular/ssr": "19.2.0",
    "@ng-bootstrap/ng-bootstrap": "18.0.0",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.3",
    "express": "4.21.2",
    "font-awesome": "4.7.0",
    "rxjs": "7.8.2",
    "shiki": "3.0.0",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.0",
    "@angular/cli": "19.2.0",
    "@angular/compiler-cli": "19.2.0",
    "@angular/localize": "19.2.0",
    "@compodoc/compodoc": "1.1.26",
    "@eslint/js": "9.21.0",
    "@types/express": "5.0.0",
    "@types/jasmine": "5.1.7",
    "@types/node": "22.13.5",
    "angular-eslint": "19.1.0",
    "commit-and-tag-version": "12.5.0",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-plugin-prettier": "5.2.3",
    "jasmine-core": "5.6.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "19.2.0",
    "ngx-speculoos": "13.1.0",
    "prettier": "3.5.2",
    "typescript": "5.6.3",
    "typescript-eslint": "8.25.0"
  }
}
