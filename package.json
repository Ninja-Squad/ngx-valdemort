{
  "name": "ngx-valdemort",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --project ngx-valdemort",
    "watch": "ng build --project ngx-valdemort --watch --configuration development",
    "test": "ng test --no-progress --no-watch --code-coverage --project ngx-valdemort",
    "build:demo": "ng build --project demo",
    "test:demo": "ng test --no-progress --no-watch --project demo",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "node scripts/prepare-doc.js && cd projects/ngx-valdemort && compodoc",
    "release": "cd projects/ngx-valdemort && commit-and-tag-version --infile ../../CHANGELOG.md",
    "format": "prettier --write ./"
  },
  "private": true,
  "packageManager": "pnpm@9.13.2",
  "dependencies": {
    "@angular/animations": "18.2.12",
    "@angular/cdk": "18.2.14",
    "@angular/common": "18.2.12",
    "@angular/compiler": "18.2.12",
    "@angular/core": "18.2.12",
    "@angular/forms": "18.2.12",
    "@angular/material": "18.2.14",
    "@angular/platform-browser": "18.2.12",
    "@angular/platform-browser-dynamic": "18.2.12",
    "@angular/platform-server": "18.2.12",
    "@angular/router": "18.2.12",
    "@angular/ssr": "18.2.12",
    "@ng-bootstrap/ng-bootstrap": "17.0.1",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.3",
    "express": "4.21.1",
    "font-awesome": "4.7.0",
    "rxjs": "7.8.1",
    "shiki": "1.23.1",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.12",
    "@angular/cli": "18.2.12",
    "@angular/compiler-cli": "18.2.12",
    "@angular/localize": "18.2.12",
    "@compodoc/compodoc": "1.1.26",
    "@types/express": "5.0.0",
    "@types/jasmine": "5.1.4",
    "@types/node": "22.9.1",
    "angular-eslint": "18.4.1",
    "commit-and-tag-version": "12.5.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jasmine-core": "5.4.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "18.2.1",
    "ngx-speculoos": "12.0.1",
    "prettier": "3.3.3",
    "typescript": "5.4.5",
    "typescript-eslint": "8.15.0"
  }
}
