{
  "name": "ngx-valdemort",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --project ngx-valdemort",
    "watch": "ng build --project ngx-valdemort --watch --configuration development",
    "test": "ng test --no-progress --no-watch --code-coverage --project ngx-valdemort",
    "build:demo": "ng build --project demo",
    "test:demo": "ng test --no-progress --no-watch --project demo",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "node scripts/prepare-doc.js && cd projects/ngx-valdemort && compodoc",
    "release": "cd projects/ngx-valdemort && commit-and-tag-version --infile ../../CHANGELOG.md",
    "format": "prettier --write ./"
  },
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@angular/cdk": "20.2.0",
    "@angular/common": "20.2.1",
    "@angular/compiler": "20.2.1",
    "@angular/core": "20.2.1",
    "@angular/forms": "20.2.1",
    "@angular/material": "20.2.0",
    "@angular/platform-browser": "20.2.1",
    "@angular/platform-server": "20.2.1",
    "@angular/router": "20.2.1",
    "@angular/ssr": "20.2.0",
    "@ng-bootstrap/ng-bootstrap": "19.0.1",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.8",
    "express": "5.1.0",
    "font-awesome": "4.7.0",
    "rxjs": "7.8.2",
    "shiki": "3.12.0",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.2.0",
    "@angular/cli": "20.2.0",
    "@angular/compiler-cli": "20.2.1",
    "@angular/localize": "20.2.1",
    "@angular-eslint/builder": "20.2.0",
    "@compodoc/compodoc": "1.1.26",
    "@eslint/js": "9.34.0",
    "@types/express": "5.0.3",
    "@types/jasmine": "5.1.9",
    "@types/node": "22.18.0",
    "angular-eslint": "20.2.0",
    "commit-and-tag-version": "12.5.2",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "jasmine-core": "5.9.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "20.2.0",
    "ngx-speculoos": "14.0.0",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.41.0"
  }
}
