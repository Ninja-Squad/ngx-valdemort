{
  "name": "ngx-valdemort",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --project ngx-valdemort",
    "watch": "ng build --project ngx-valdemort --watch --configuration development",
    "test": "ng test --no-progress --no-watch --code-coverage --project ngx-valdemort",
    "build:demo": "ng build --project demo",
    "test:demo": "ng test --no-progress --no-watch --project demo",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "node scripts/prepare-doc.js && cd projects/ngx-valdemort && compodoc",
    "release": "cd projects/ngx-valdemort && commit-and-tag-version --infile ../../CHANGELOG.md",
    "format": "prettier --write ./"
  },
  "private": true,
  "packageManager": "pnpm@10.30.0",
  "dependencies": {
    "@angular/cdk": "21.1.5",
    "@angular/common": "21.1.5",
    "@angular/compiler": "21.1.5",
    "@angular/core": "21.1.5",
    "@angular/forms": "21.1.5",
    "@angular/material": "21.1.5",
    "@angular/platform-browser": "21.1.5",
    "@angular/platform-server": "21.1.5",
    "@angular/router": "21.1.5",
    "@angular/ssr": "21.1.4",
    "@ng-bootstrap/ng-bootstrap": "20.0.0",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.8",
    "express": "5.2.1",
    "font-awesome": "4.7.0",
    "rxjs": "7.8.2",
    "shiki": "3.22.0",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.2.0",
    "@angular/build": "21.1.4",
    "@angular/cli": "21.1.4",
    "@angular/compiler-cli": "21.1.5",
    "@angular/localize": "21.1.5",
    "@compodoc/compodoc": "1.2.1",
    "@eslint/js": "9.39.2",
    "@types/express": "5.0.6",
    "@types/jasmine": "5.1.15",
    "@types/node": "24.10.13",
    "angular-eslint": "21.2.0",
    "commit-and-tag-version": "12.6.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "jasmine-core": "5.13.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.2.0",
    "ng-packagr": "21.1.0",
    "ngx-speculoos": "15.0.0",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0"
  }
}
