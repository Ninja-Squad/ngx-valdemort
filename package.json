{
  "name": "ngx-valdemort",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --project ngx-valdemort",
    "watch": "ng build --project ngx-valdemort --watch --configuration development",
    "test": "ng test --no-progress --no-watch --code-coverage --project ngx-valdemort",
    "build:demo": "ng build --project demo",
    "test:demo": "ng test --no-progress --no-watch --project demo",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "codecov": "codecov",
    "doc": "node scripts/prepare-doc.js && cd projects/ngx-valdemort && compodoc",
    "release": "cd projects/ngx-valdemort && standard-version --infile ../../CHANGELOG.md",
    "format": "prettier --write ./"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.9",
    "@angular/cdk": "17.3.9",
    "@angular/common": "17.3.9",
    "@angular/compiler": "17.3.9",
    "@angular/core": "17.3.9",
    "@angular/forms": "17.3.9",
    "@angular/material": "17.3.9",
    "@angular/platform-browser": "17.3.9",
    "@angular/platform-browser-dynamic": "17.3.9",
    "@angular/platform-server": "17.3.9",
    "@angular/router": "17.3.9",
    "@angular/ssr": "17.3.7",
    "@ng-bootstrap/ng-bootstrap": "16.0.0",
    "@popperjs/core": "2.11.8",
    "bootstrap": "5.3.3",
    "express": "4.19.2",
    "font-awesome": "4.7.0",
    "prismjs": "1.29.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.3.7",
    "@angular-eslint/builder": "17.5.0",
    "@angular-eslint/eslint-plugin": "17.5.0",
    "@angular-eslint/eslint-plugin-template": "17.5.0",
    "@angular-eslint/template-parser": "17.5.0",
    "@angular/cli": "17.3.7",
    "@angular/compiler-cli": "17.3.9",
    "@angular/localize": "17.3.9",
    "@compodoc/compodoc": "1.1.24",
    "@types/express": "4.17.21",
    "@types/jasmine": "5.1.4",
    "@types/node": "18.19.33",
    "@types/prismjs": "1.26.4",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "codecov": "3.8.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
