    "codecov": "codecov",
    "doc": "node scripts/prepare-doc.js && cd projects/ngx-valdemort && compodoc",
    "release": "cd projects/ngx-valdemort && standard-version --infile ../../CHANGELOG.md",
    "format": "prettier --write ./"
